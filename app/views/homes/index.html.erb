<div class="row col-xs-offset-1"><h5>Listings</h5></div>

<% @homes.each do |home| %>
    <div class="booyah-box col-xs-10 col-xs-offset-1">
      <h3><%= home.address %><br />
      <%= home.city%>, <%= home.state %> <%= home.zipcode %></h3><br />
      $<%= home.listingprice %><br />
    </div>
<% end %>

<div class="row">
    <div class="col-xs-4 col-xs-offset-8">
      <%= will_paginate @homes %>
    </div>
</div>